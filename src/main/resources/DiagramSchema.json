{
   "$schema":"https://json-schema.org/draft/2019-09/schema",
   "$defs":{
      "DiffDto":{
         "type":"object",
         "properties":{
            "conflict":{
               "type":["object","null"],
               "properties":{
                  "diffs":{
                     "type":"array",
                     "items":{
                        "$ref":"#/$defs/DiffDto"
                     }
                  },
                  "kind":{
                     "$ref":"#/$defs/KindDto"
                  },
                  "leftDiffs":{
                     "type":"array",
                     "items":{
                        "$ref":"#/$defs/DiffDto"
                     }
                  },
                  "rightDiffs":{
                     "type":"array",
                     "items":{
                        "$ref":"#/$defs/DiffDto"
                     }
                  },
                  "type":{
                     "$ref":"#/$defs/TypeDto"
                  }
               }
            },
            "kind":{
               "$ref":"#/$defs/KindDto"
            },
            "referenceDto":{
               "type":"object",
               "properties":{
                  "lowerBound":{
                     "type":"integer"
                  },
                  "name":{
                     "type":"string"
                  },
                  "upperBound":{
                     "type":"integer"
                  }
               }
            },
            "type":{
               "$ref":"#/$defs/TypeDto"
            },
            "value":{
               "type":["string", "null"]
            }
         }
      },
      "KindDto":{
         "type":"string",
         "enum":[
            "ADD",
            "DELETE",
            "CHANGE",
            "MOVE"
         ]
      },
      "MatchDto":{
         "type":"object",
         "properties":{
            "diffs":{
               "type":"array",
               "items":{
                  "$ref":"#/$defs/DiffDto"
               }
            },
            "left":{
               "$ref":"#/$defs/ModelElementDto"
            },
            "origin":{
               "$ref":"#/$defs/ModelElementDto"
            },
            "right":{
               "$ref":"#/$defs/ModelElementDto"
            },
            "subMatches":{
               "type":"array",
               "items":{
                  "$ref":"#/$defs/MatchDto"
               }
            }
         }
      },
      "ModelElementDto":{
         "type":["object","null"],
         "properties":{
            "name":{
               "type":["string", "null"]
            },
            "target":{
               "type":["string", "null"]
            }
         }
      },
      "TypeDto":{
         "type":"string",
         "enum":[
            "ATTRIBUTECHANGE",
            "REFERENCECHANGE",
            "RESOURCEATTACHMENTCHANGE"
         ]
      }
   },
   "type":"object",
   "properties":{
      "matches":{
         "type":"array",
         "items":{
            "$ref":"#/$defs/MatchDto"
         }
      },
      "threeWay":{
         "type":"boolean"
      }
   }
}
